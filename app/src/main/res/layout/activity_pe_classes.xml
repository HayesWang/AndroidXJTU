<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="peClassesVM" type="com.xjtusports.app.modules.peclasses.data.viewmodel.PeClassesVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="end"
            android:id="@+id/linearPEClasses"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvector"
                android:layout_height="@dimen/_768pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackjan"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_768pxv" android:layout_width="@dimen/_353pxh">
                    <LinearLayout android:id="@+id/linearColumnjan"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <androidx.recyclerview.widget.RecyclerView
                            android:gravity="center_horizontal"
                            android:id="@+id/recyclerPeClasses"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_22pxh}"
                            tools:itemCount="2" tools:listitem="@layout/row_pe_classes"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnSignInForTodaysLesson"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_43pxv"
                            android:layout_width="@dimen/_222pxh"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_9pxh"
                            android:paddingStart="@dimen/_11pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@string/msg_sign_in_for_tod"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_3" tools:text="@string/msg_sign_in_for_tod"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageOne"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_189pxh"
                        android:layout_width="@dimen/_189pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_1"/>
                    <TextView android:id="@+id/txtPEClasses"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{peClassesVM.peClassesModel.txtPEClasses}"
                        style="@style/txtRubikitalicsemibold36" tools:text="@string/lbl_pe_classes"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_42pxh"
                    android:layout_width="@dimen/_42pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_gray_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_700"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_500cornerRadius">
                <EditText
                    android:background="@drawable/rectangle_bg_indigo_a402_radius_16"
                    android:drawablePadding="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_home"
                    android:id="@+id/etHome" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_5pxv"
                    android:paddingEnd="@dimen/_10pxh"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@={peClassesVM.peClassesModel.etHomeValue}"
                    android:textColorHint="@color/white_A700"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <ImageView android:id="@+id/imageExplore"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_explore}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_explore"/>
                <ImageView android:id="@+id/imageInfo"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_info_white_a700}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_info_white_a700"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
