<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dhi0VM" type="com.xjtusports.app.modules.dhi0.data.viewmodel.Dhi0VM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackwelcome"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnwelcome"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarNavigationbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearInfo"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowwelcome"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageImage"
                            android:layout_height="@dimen/_53pxh"
                            android:layout_width="@dimen/_53pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_image}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearInfo1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtWelcome"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{dhi0VM.dhi0Model.txtWelcome}"
                                style="@style/txtHYQiHeis16" tools:text="@string/lbl_welcome"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtPeterHanson"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{dhi0VM.dhi0Model.txtPeterHanson}"
                                style="@style/txtRubikromanbold20" tools:text="@string/lbl_peter_hanson"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowyoursemesterp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnyoursemesterp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:id="@+id/txtYourSemesterP"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{dhi0VM.dhi0Model.txtYourSemesterP}"
                        style="@style/txtRubikitalicextrabold23" tools:text="@string/msg_your_semester_p"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnExplore"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_width="@dimen/_99pxh"
                        android:paddingBottom="@dimen/_5pxv"
                        android:paddingEnd="@dimen/_20pxh"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_5pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_explore"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_explore"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTwentyFive"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{dhi0VM.dhi0Model.txtTwentyFive}"
                    style="@style/txtRubikOneregular36" tools:text="@string/lbl_25"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtShortcut"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{dhi0VM.dhi0Model.txtShortcut}"
                style="@style/txtRubikromanbold15" tools:text="@string/lbl_shortcut"/>
            <HorizontalScrollView android:fillViewport="true"
                android:id="@+id/horizontalScrollHorizontalscrollstart"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:scrollbars="none">
                <FrameLayout android:id="@+id/frameStack"
                    android:layout_height="@dimen/_411pxv" android:layout_width="@dimen/_363pxh">
                    <com.asksira.loopingviewpager.LoopingViewPager
                        android:id="@+id/imageSliderSliderstart"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_368pxv"
                        android:layout_width="match_parent"
                        app:autoScroll="true" app:isInfinite="true" app:scrollInterval="5000"/>
                    <LinearLayout android:id="@+id/linearShortcut"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                        <FrameLayout
                            android:id="@+id/frameStackfreeexercise"
                            android:layout_height="@dimen/_41pxv"
                            android:layout_width="@dimen/_143pxh" style="@style/groupStylecornerRadius_1">
                            <TextView android:ellipsize="end"
                                android:gravity="top|start"
                                android:id="@+id/txtFreeexercise"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{dhi0VM.dhi0Model.txtFreeexercise}"
                                style="@style/txtRubikromanbold16" tools:text="@string/lbl_free_exercise"/>
                            <View
                                android:background="@drawable/rectangle_bg_bluegray_100_05_border_black_900_radius_20"
                                android:id="@+id/viewFreeexerciseOne"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_41pxv" android:layout_width="@dimen/_143pxh"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frameStacksignin"
                            android:layout_height="@dimen/_41pxv"
                            android:layout_width="@dimen/_143pxh" style="@style/groupStylecornerRadius_1">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSignIn"
                                android:layout_gravity="bottom|center"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{dhi0VM.dhi0Model.txtSignIn}"
                                style="@style/txtRubikromanbold16" tools:text="@string/lbl_sign_in"/>
                            <View
                                android:background="@drawable/rectangle_bg_bluegray_100_05_border_black_900_radius_20"
                                android:id="@+id/viewSigninOne"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_41pxv" android:layout_width="@dimen/_143pxh"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frameStackadvice"
                            android:layout_height="@dimen/_41pxv"
                            android:layout_width="@dimen/_143pxh" style="@style/groupStylecornerRadius_1">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtAdvice"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{dhi0VM.dhi0Model.txtAdvice}"
                                style="@style/txtRubikromanbold16" tools:text="@string/lbl_advice"/>
                            <ImageView android:id="@+id/imageAdviceOne"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_41pxh"
                                android:layout_width="@dimen/_143pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_advice"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_advice"/>
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>
            </HorizontalScrollView>
            <FrameLayout android:id="@+id/frameStackrectanglefifteen"
                android:layout_height="@dimen/_14pxv" android:layout_width="@dimen/_113pxh">
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_7"
                    android:id="@+id/viewRectangleFifteen"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_14pxv" android:layout_width="@dimen/_113pxh"/>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorGroup280"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    app:drawable_normal="@drawable/indicator_unselected_bg_white_a700_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_red_300_radius_8"
                    app:indicator_size="@dimen/_8pxh" app:indicator_spacing="@dimen/_33pxh"/>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
