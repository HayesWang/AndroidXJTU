<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="freeExerciseVM" type="com.xjtusports.app.modules.freeexercise.data.viewmodel.FreeExerciseVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackyoursemesterp"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameFreeexercise"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearSemesterprogre"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:gravity="start"
                    android:id="@+id/txtYourSemesterP"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:text="@{freeExerciseVM.freeExerciseModel.txtYourSemesterP}"
                    style="@style/txtRubikitalicextrabold23" tools:text="@string/msg_your_semester_p"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearContent"
                android:layout_gravity="bottom|end"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameStack33878"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_693pxv" android:layout_width="@dimen/_348pxh">
                    <FrameLayout
                        android:id="@+id/frameStackrectangleeleven"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_640pxv"
                        android:layout_width="@dimen/_336pxh" style="@style/groupStylewhite_A700cornerRadius">
                        <View
                            android:background="@drawable/rectangle_bg_deep_orange_50_radius_20"
                            android:id="@+id/viewRectangleEleven"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_169pxv" android:layout_width="@dimen/_280pxh"/>
                        <LinearLayout
                            android:id="@+id/linearColumnpresetroute"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtPresetRoute"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@{freeExerciseVM.freeExerciseModel.txtPresetRoute}"
                                style="@style/txtRubikitalicsemibold24" tools:text="@string/lbl_preset_route"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtDistance"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{freeExerciseVM.freeExerciseModel.txtDistance}"
                                style="@style/txtRubikromanregular15" tools:text="@string/msg_follow_certain"/>
                            <FrameLayout
                                android:id="@+id/frameStackfreeexercise"
                                android:layout_height="@dimen/_192pxv" android:layout_width="@dimen/_280pxh">
                                <LinearLayout
                                    android:id="@+id/linearColumnfreeexercise"
                                    android:layout_gravity="bottom|start"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStylered_100cornerRadius">
                                    <TextView android:ellipsize="end"
                                    android:id="@+id/txtFreeExercise"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{freeExerciseVM.freeExerciseModel.txtFreeExercise}"
                                    style="@style/txtRubikitalicsemibold24" tools:text="@string/lbl_free_exercise2"/>
                                    <TextView android:ellipsize="end"
                                    android:id="@+id/txtDistanceOne"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:singleLine="true"
                                    android:text="@{freeExerciseVM.freeExerciseModel.txtDistanceOne}"
                                    style="@style/txtRubikromanregular15" tools:text="@string/msg_exercise_freely"/>
                                </LinearLayout>
                                <ImageView android:id="@+id/imageOne"
                                    android:layout_gravity="top|end"
                                    android:layout_height="@dimen/_128pxh"
                                    android:layout_width="@dimen/_128pxh"
                                    android:scaleType="fitXY"
                                    imageRes="@{@drawable/img_1_128x128}"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_1_128x128"/>
                            </FrameLayout>
                        </LinearLayout>
                    </FrameLayout>
                    <ImageView android:id="@+id/image33878"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_205pxh"
                        android:layout_width="@dimen/_205pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_33878}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_33878"/>
                    <TextView android:id="@+id/txtNewExercise"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{freeExerciseVM.freeExerciseModel.txtNewExercise}"
                        style="@style/txtRubikitalicsemibold36" tools:text="@string/lbl_new_exercise"/>
                    <ImageView android:id="@+id/image444330"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_128pxh"
                        android:layout_width="@dimen/_128pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_444330}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_444330"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackagenda"
                    android:layout_height="@dimen/_189pxv" android:layout_width="@dimen/_340pxh">
                    <LinearLayout android:id="@+id/linearColumn"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnagenda"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtAgenda"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{freeExerciseVM.freeExerciseModel.txtAgenda}"
                                style="@style/txtRubikitalicbold16" tools:text="@string/lbl_agenda"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageOneOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_189pxh"
                        android:layout_width="@dimen/_189pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_1"/>
                    <TextView android:id="@+id/txtPEClasses"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{freeExerciseVM.freeExerciseModel.txtPEClasses}"
                        style="@style/txtRubikitalicsemibold36" tools:text="@string/lbl_pe_classes"/>
                </FrameLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageVector"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_42pxh"
                android:layout_width="@dimen/_42pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_700"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_700"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTwentyFive"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{freeExerciseVM.freeExerciseModel.txtTwentyFive}"
                style="@style/txtRubikOneregular36" tools:text="@string/lbl_25"/>
            <FrameLayout android:id="@+id/frameStackagendaOne"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_205pxv" android:layout_width="@dimen/_348pxh">
                <LinearLayout android:id="@+id/linearColumnOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnagendaOne"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAgendaOne"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{freeExerciseVM.freeExerciseModel.txtAgendaOne}"
                            style="@style/txtRubikitalicbold16" tools:text="@string/lbl_agenda"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/image33878One"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_205pxh"
                    android:layout_width="@dimen/_205pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_33878}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_33878"/>
                <TextView android:id="@+id/txtPEClassesOne"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:text="@{freeExerciseVM.freeExerciseModel.txtPEClassesOne}"
                    style="@style/txtRubikitalicsemibold36" tools:text="@string/lbl_pe_classes"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_500cornerRadius">
                <EditText
                    android:background="@drawable/rectangle_bg_indigo_a402_radius_16"
                    android:drawablePadding="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_home"
                    android:id="@+id/etHome" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_5pxv"
                    android:paddingEnd="@dimen/_10pxh"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@={freeExerciseVM.freeExerciseModel.etHomeValue}"
                    android:textColorHint="@color/white_A700"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <ImageView android:id="@+id/imageExplore"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_explore}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_explore"/>
                <ImageView android:id="@+id/imageInfo"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_info_white_a700}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_info_white_a700"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
